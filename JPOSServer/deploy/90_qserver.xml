<qserver name="xml-server" logger="Q2">
    <attr name="port" type="java.lang.Integer">5000</attr>
    <channel name="xml.channel"
            class="org.jpos.iso.channel.ASCIIChannel"
            packager="org.jpos.iso.packager.ISO93APackager"
           />
    <request-listener class="com.pkteq.JPOSServer.Server" logger="Q2">
        <property name="space" value="transient:default"/>
        <property name="queue" value="TXNMGR"/>
        <property name="timeout" value="12000"/>
    </request-listener>
</qserver>